{
  "name": "@sudobility/ratelimit_service",
  "version": "1.0.10",
  "description": "Shared rate limiting library based on RevenueCat entitlements",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./middleware/hono": {
      "import": "./dist/middleware/hono.js",
      "types": "./dist/middleware/hono.d.ts"
    },
    "./schema": {
      "import": "./dist/schema/rate-limits.js",
      "types": "./dist/schema/rate-limits.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.esm.json",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "verify": "bun run typecheck && bun run lint && bun run test && bun run build",
    "prepublishOnly": "bun run clean && bun run verify"
  },
  "files": [
    "dist/**/*",
    "CLAUDE.md"
  ],
  "keywords": [
    "rate-limiting",
    "revenuecat",
    "subscription",
    "entitlements",
    "hono",
    "drizzle"
  ],
  "author": "Sudobility",
  "license": "MIT",
  "peerDependencies": {
    "@sudobility/types": "^1.9.43",
    "drizzle-orm": "^0.45.0",
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "vitest": "^4.0.4",
    "@sudobility/types": "^1.9.43",
    "@types/bun": "latest",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "drizzle-orm": "^0.45.0",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "hono": "^4.10.7",
    "postgres": "^3.4.7",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
